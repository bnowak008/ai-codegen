{
    "env": {
      "es6": true,
      "node": true
    },
    "extends": ["airbnb-base", "plugin:security/recommended", "plugin:prettier/recommended"],
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "plugins": ["prettier", "security"],
    "rules": {
      "prettier/prettier": ["error"],
      "prefer-destructuring": ["error", { "VariableDeclarator": { "object": true } }]
    }
}